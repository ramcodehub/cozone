{
  "name": "cozone-backend",
  "version": "1.0.0",
  "description": "Backend for CoZone AI assistant",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "postinstall": "npx rimraf node_modules/debug node_modules/express node_modules/path-to-regexp && npm install express@^4.18.2 path-to-regexp@^6.2.1 debug@latest",
    "migrate": "node scripts/runMigrations.js",
    "setup-tables": "node scripts/setupTables.js",
    "test-tables": "node scripts/createTestRecords.js"
  },
  "engines": {
    "node": "20.10.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.87.0",
    "axios": "^1.13.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "cozone",
    "backend"
  ],
  "author": "CoZone Team",
  "license": "MIT"
}
